
# https://github.com/rebuy-de/aws-nuke  
# https://github.com/rebuy-de/aws-nuke/releases



aws-nuke-v2.14.0-linux-amd64 -c config.yml --profile default --no-dry-run



config.yml
---
regions:
  - "us-east-1"
  - "us-east-2"
  - "us-west-1"
  - "us-west-2"
  - "ap-east-1"
  - "ap-south-1"
  - "ap-north-east-2"
  - "ap-south-east-1"
  - "ap-south-east-2"
  - "ap-northeast-1"
  - "ca-central-1"
  - "eu-central-1"
  - "eu-west-1"
  - "eu-west-2"
  - "eu-west-3"
  - "eu-north-1"
  - "me-south-1"
  - "sa-east-1"
account-blacklist:
- 1234567890

resource-types:
  # don't nuke IAM users
  excludes:
  - IAMUser

accounts:
  PUT-YOUR-ACCOUNT-NUMBER-HERE-FOR-DESTRUCTION: {}
